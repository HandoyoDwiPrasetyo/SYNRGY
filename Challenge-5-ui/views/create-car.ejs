<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>

    <!-- Bootstrap -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      crossorigin="anonymous"
    />

    <!-- Custom CSS -->
    <link rel="stylesheet" href="./styles/list-car.css" />
  </head>
  <body>
    <nav class="navbar shadow-sm bg-white main-content" id="main-navbar">
      <div class="container-fluid">
        <button
          class="navbar-toggler border-0"
          type="button"
          data-bs-toggle="offcanvas"
          data-bs-target="#offcanvasScrolling"
          aria-controls="offcanvasScrolling"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <form class="d-flex ms-auto" role="search">
          <input
            class="form-control me-1 rounded-1"
            type="search"
            placeholder="Search"
            aria-label="Search"
          />
          <button class="btn btn-outline-primary rounded-1" type="submit">
            Search
          </button>
        </form>
        <li
          class="nav-item dropdown ms-4 me-2 list-unstyled d-flex align-items-center"
        >
          <div
            class="rounded-circle second-color text-white fw-bold text-center py-1 me-2"
            style="width: 2rem; height: 2rem"
          >
            U
          </div>
          <a
            class="nav-link dropdown-toggle"
            href="#"
            role="button"
            data-bs-toggle="dropdown"
            aria-expanded="false"
          >
            Unis Badri
          </a>
          <ul class="dropdown-menu">
            <li>
              <a class="dropdown-item" href="#">Action</a>
            </li>
          </ul>
        </li>
      </div>
    </nav>
    <div
      class="offcanvas offcanvas-start show offcanvas-submenu border-0"
      id="offcanvas-submenu"
      data-bs-scroll="true"
      data-bs-backdrop="false"
    >
      <div class="" style="height: 3.375rem"></div>

      <div class="" style="margin-left: 4.375rem; margin-top: 6rem">
        <p class="px-3 fw-bold text-secondary">CARS</p>
        <div class="py-2 bg-body-secondary">
          <p class="px-3 fw-bold my-auto">List Car</p>
        </div>
      </div>
    </div>

    <div
      class="offcanvas offcanvas-start show offcanvas-main main-color"
      id="offcanvas-menu"
      data-bs-scroll="true"
      data-bs-backdrop="false"
    >
      <div class="box-main second-color mx-auto my-3"></div>
      <button class="btn main-menu border-0 p-0 rounded-0">
        <div>
          <svg
            xmlns="http://www.w3.org/2000/svg"
            width="24"
            height="24"
            viewBox="0 0 24 24"
            fill="none"
          >
            <path
              d="M3 9L12 2L21 9V20C21 20.5304 20.7893 21.0391 20.4142 21.4142C20.0391 21.7893 19.5304 22 19 22H5C4.46957 22 3.96086 21.7893 3.58579 21.4142C3.21071 21.0391 3 20.5304 3 20V9Z"
              stroke="white"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
            />
            <path
              d="M9 22V12H15V22"
              stroke="white"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
            />
          </svg>
        </div>
        <div class="mt-1">Dashboard</div>
      </button>
      <button class="btn main-menu border-0 p-0 rounded-0">
        <div>
          <svg
            xmlns="http://www.w3.org/2000/svg"
            width="24"
            height="24"
            viewBox="0 0 24 24"
            fill="none"
          >
            <path
              d="M18.5 21C19.8807 21 21 19.8807 21 18.5C21 17.1193 19.8807 16 18.5 16C17.1193 16 16 17.1193 16 18.5C16 19.8807 17.1193 21 18.5 21Z"
              stroke="white"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
            />
            <path
              d="M5.5 21C6.88071 21 8 19.8807 8 18.5C8 17.1193 6.88071 16 5.5 16C4.11929 16 3 17.1193 3 18.5C3 19.8807 4.11929 21 5.5 21Z"
              stroke="white"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
            />
            <path
              d="M16 8H20L23 11V16H16V8Z"
              stroke="white"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
            />
            <path
              d="M16 3H1V16H16V3Z"
              stroke="white"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
            />
          </svg>
        </div>
        <div class="mt-1">Cars</div>
      </button>
    </div>

    <section id="list-car" class="my-4 main-content px-4">
      <div class="">
        <p><span class="fw-bold">Cars > List Car ></span> Add New Car</p>
        <div class="row">
          <div class="col">
            <h3 class="fw-bold my-auto">Add New Car</h3>
          </div>
        </div>
      </div>

      <div class="row mt-4">
        <form>
          <div class="card border-0 py-4 px-3 mb-5">
            <div class="row">
              <div class="col-2">
                <label class="form-label"
                  >Nama <span class="text-danger">*</span></label
                >
              </div>
              <div class="col-5">
                <input
                  type="text"
                  class="form-control"
                  id="carname"
                  aria-describedby="emailHelp"
                />
              </div>
              <div class="col-5"></div>
            </div>
            <div class="row mt-3">
              <div class="col-2">
                <label class="form-label"
                  >Harga <span class="text-danger">*</span></label
                >
              </div>
              <div class="col-5">
                <input
                  type="number"
                  class="form-control"
                  id="price"
                  aria-describedby="emailHelp"
                />
              </div>
              <div class="col-5"></div>
            </div>
            <div class="row mt-3">
              <div class="col-2">
                <label class="form-label"
                  >Foto <span class="text-danger">*</span></label
                >
              </div>
              <div class="col-5">
                <input
                  type="text"
                  class="form-control"
                  id="carname"
                  aria-describedby="emailHelp"
                />
                <div id="emailHelp" class="form-text">File size max. 2MB</div>
              </div>
              <div class="col-5"></div>
            </div>
            <div class="row mt-3">
              <div class="col-2">
                <label class="form-label">Start Rent </label>
              </div>
              <div class="col-5">-</div>
              <div class="col-5"></div>
            </div>
            <div class="row mt-3">
              <div class="col-2">
                <label class="form-label">Finish Rent </label>
              </div>
              <div class="col-5">-</div>
              <div class="col-5"></div>
            </div>
            <div class="row mt-3">
              <div class="col-2">
                <label class="form-label">Create At </label>
              </div>
              <div class="col-5">-</div>
              <div class="col-5"></div>
            </div>
            <div class="row mt-3">
              <div class="col-2">
                <label class="form-label">Update At </label>
              </div>
              <div class="col-5">-</div>
              <div class="col-5"></div>
            </div>
          </div>

          <button type="button" class="btn btn-outline-primary rounded-1">
            Cancel
          </button>
          <button type="button" class="btn btn-primary rounded-1 ms-2">
            Save
          </button>
        </form>
      </div>
    </section>

    <!-- Bootstrap JS -->
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
      crossorigin="anonymous"
    ></script>

    <!-- Custom JS -->
    <script src="./scripts/list-car.js"></script>
  </body>
</html>
